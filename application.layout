    <script>
    
        //Using i18n for localization, for more info please visit http://i18next.com/
        i18n.init({preload: ['en-CA','fr-CA'],resGetPath: '__lng__.json'}, function(t) {
            //load en-CA by default
            i18n.setLng('en-CA', function(t) {
                $(document).i18n();
            });
        });
        
        $('#en-CA').click(function(){
            event.preventDefault();
            i18n.setLng('en-CA', function(t) {
                $(document).i18n();
            });
        });
        
        $('#fr-CA').click(function(){
            event.preventDefault();
            i18n.setLng('fr-CA', function(t) {
                $(document).i18n();
            });
        });
        
        
        
        $('#search_results').hide();
        $('#search_input').on('input', function() {
            $('#search_results').show();
            if($('#search_input').val().length == 0){
                $('#search_results').hide();
            }else{
                renderSearchResultsTemplate('#search_results_items',$('#search_input').val(),10);
            }
        });
        
        $('#search_input').bind('blur', function(){
            //$('#search_results').hide();
        });
        
        
    </script>
